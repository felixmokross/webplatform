{
  "name": "@fxmk/cms-plugin",
  "version": "0.0.0-unknown",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm copyfiles && pnpm build:types && pnpm build:swc",
    "build:swc": "swc ./src -d ./dist --config-file .swcrc --strip-leading-paths",
    "build:types": "tsc --outDir dist --rootDir ./src",
    "clean": "rimraf {dist,*.tsbuildinfo}",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,json}\" dist/",
    "generate:importmap": "pnpm dev:generate-importmap",
    "generate:types": "pnpm dev:generate-types",
    "lint": "eslint",
    "lint:fix": "eslint ./src --fix",
    "test": "pnpm test:int && pnpm test:e2e",
    "test:e2e": "playwright test",
    "test:int": "vitest",
    "check-format": "prettier --check ."
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@payloadcms/db-mongodb": "3.50.0",
    "@payloadcms/eslint-config": "3.9.0",
    "@payloadcms/richtext-lexical": "^3.50.0",
    "@payloadcms/translations": "^3.50.0",
    "@payloadcms/ui": "^3.50.0",
    "@swc-node/register": "1.10.9",
    "@swc/cli": "0.6.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.5.4",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "bson": "^6.10.4",
    "copyfiles": "2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.23.0",
    "eslint-config-next": "15.4.4",
    "jsdom": "26.1.0",
    "payload": "^3.50.0",
    "prettier": "^3.4.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "rimraf": "3.0.2",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.2"
  },
  "peerDependencies": {
    "@payloadcms/db-mongodb": "3.50.0",
    "@payloadcms/richtext-lexical": "^3.50.0",
    "@payloadcms/translations": "^3.50.0",
    "@payloadcms/ui": "^3.50.0",
    "bson": "^6.10.4",
    "jsdom": "26.1.0",
    "payload": "^3.50.0",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./client": {
      "import": "./dist/components/client/index.js",
      "types": "./dist/components/client/index.d.ts",
      "default": "./dist/components/client/index.js"
    },
    "./rsc": {
      "import": "./dist/components/rsc/index.js",
      "types": "./dist/components/rsc/index.d.ts",
      "default": "./dist/components/rsc/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "registry": "https://registry.npmjs.org/",
  "dependencies": {
    "deepl-node": "^1.19.0"
  }
}
